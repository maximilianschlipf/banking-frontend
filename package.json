{
  "name": "banking-software-front-end",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --localize --configuration=production",
    "cypress:open": "cypress open",
    "test:smoketests": "cypress run --env tags=\"@Smoketest\" --config baseUrl=http://app.testorbit.de:4200/ --reporter junit --reporter-options \"mochaFile=results/smoketests-[suiteName].xml\"",
    "test:e2e": "cypress run --env tags=\"not @Smoketest\" --reporter junit --reporter-options \"mochaFile=results/e2e-[suiteName].xml\"",
    "test:int": "cypress run --component --reporter junit --reporter-options \"mochaFile=results/integration-[suiteName].xml\"",
    "start": "ng serve",
    "clear-cypress-cache": "cypress cache clear"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.8",
    "@angular/cdk": "^16.2.7",
    "@angular/common": "^16.2.8",
    "@angular/compiler": "^16.2.8",
    "@angular/core": "^16.2.8",
    "@angular/forms": "^16.2.8",
    "@angular/localize": "^16.2.8",
    "@angular/material": "^16.2.7",
    "@angular/platform-browser": "^16.2.8",
    "@angular/platform-browser-dynamic": "^16.2.8",
    "@angular/router": "^16.2.8",
    "@cucumber/cucumber": "^10.3.1",
    "@cypress/code-coverage": "^3",
    "@fortawesome/angular-fontawesome": "~0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@ng-bootstrap/ng-bootstrap": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "axe-core": "^4.8.3",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.2",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.5.0",
    "enhanced-resolve": "3.3.0",
    "eslint": "^8.54.0",
    "font-awesome": "^4.7.0",
    "js-cookie": "^3.0.5",
    "rxjs": "~6.5.5",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/build-angular": "^16.2.5",
    "@angular/cli": "^16.2.5",
    "@angular/compiler-cli": "^16.2.8",
    "@badeball/cypress-cucumber-preprocessor": "^19.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/schematic": "2.5.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^14.18.63",
    "babel-plugin-istanbul": "^6.1.1",
    "eslint-plugin-cypress": "^2.15.1",
    "istanbul-lib-coverage": "^3.2.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "nyc": "^15.1.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5",
    "webpack": "^5.90.1"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true
  }
